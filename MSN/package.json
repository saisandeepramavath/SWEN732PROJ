{
  "name": "msn",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
  "start": "expo start",
  "reset-project": "node ./scripts/reset-project.js",
  "android": "expo run:android",
  "ios": "expo run:ios",
  "web": "expo start --web",
  "test": "jest --coverage",
  "testFinal": "jest",
  "lint": "expo lint",
  "coverage": "jest --coverage",
  "sonar": "npm run coverage && sonar-scanner"
}
,
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "./jestSetup.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.tsx"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**",
      "!**/utils/**",
      "!**/state/**",
      "!**/scripts/**",
      "!**/app/splitOptions.tsx",
      "!**/app/\\(modal\\)/add.tsx",
      "!**/app/\\(modal\\)/selectPayer.tsx",
      "!**/app/group/[groupId].tsx",
      "!**/app/group/Charts.tsx",
      "!**/app/group/ExpenseDetail.tsx",
      "!**/app/group/SettleUp.tsx",
      "!**/app/group/Charts.tsx",
      "!**/app/\\(auth\\)/**",
      "!firebaseConfig.js",
      "!**/constants/**",
      "!**/__tests__/**",
      "!**/hooks/**"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|@react-navigation|@expo|expo|expo-constants|expo-font|expo-linking|expo-splash-screen|expo-status-bar|expo-symbols|expo-system-ui|expo-web-browser|expo-blur|expo-haptics|expo-router|@react-native-firebase)",
      "_layout.tsx"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-community/checkbox": "^0.5.17",
    "@react-native-firebase/app": "^21.12.0",
    "@react-native-firebase/auth": "^21.12.0",
    "@react-native-firebase/crashlytics": "^21.12.0",
    "@react-native-firebase/firestore": "^21.12.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@shopify/react-native-skia": "^1.12.4",
    "expo": "^52.0.44",
    "expo-blur": "~14.0.3",
    "expo-build-properties": "~0.13.2",
    "expo-constants": "~17.0.7",
    "expo-contacts": "~14.0.5",
    "expo-file-system": "~18.0.12",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image-picker": "~16.0.6",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-sharing": "~13.0.1",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "jest": "~29.7.0",
    "jest-expo": "~52.0.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-chart-kit": "^6.12.0",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-paper": "^5.13.1",
    "react-native-reanimated": "^3.17.4",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "recoil": "^0.7.7",
    "sonar-scanner": "^3.1.0",
    "victory-native": "^41.17.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-react": "^7.26.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "@types/xlsx": "^0.0.35",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.4",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
